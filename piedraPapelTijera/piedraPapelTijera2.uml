        <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="__Kqd46ojEeydhrAqYvfUBg" name="playGame" specification="__HgcMKojEeydhrAqYvfUBg">
          <language>java</language>
          <body>boolean continuing = true;
		Move move;
		int round = 1;
		while (continuing) {
			System.out.println(&quot;****** &quot; + round + &quot; turno ******&quot;);
			move = parser.getMove();
			continuing = processMove(move);
			round++;
		}
		System.out.println(&quot;Adios!!&quot;);
	</body>
