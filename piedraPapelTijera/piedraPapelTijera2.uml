          </ownedParameter>
          <language>java</language>
          <body>Move pcMove;

		if (move.isUnknown()) {
			System.out.println(&quot;Opcion No Valida&quot;);
			printHelp();
			return true;
		}

		if (move.getMove().equals(&quot;SALIR&quot;)) {
			return false;
		}

		if (move.getMove().equals(&quot;AYUDA&quot;)) {
			printHelp();
			return true;
		}
		pcMove = computerMove();
		printMoves(move, pcMove);
		printWinner(move.beats(pcMove));
		return true;
	</body>
